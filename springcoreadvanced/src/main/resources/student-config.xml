<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:p="http://www.springframework.org/schema/p"
       xmlns:c="http://www.springframework.org/schema/c"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context.xsd">

    <bean name="institute" class="org.dhiren.spring.annotation.models.Institute" c:id="2109334"
          c:name="IIT Delhi" />

    <bean name="pay" class="org.dhiren.spring.annotation.models.PayRoll" c:id="317097"
          c:name="15.5 LPA" />

    <bean name="subject" class="org.dhiren.spring.annotation.models.Subject" c:id="12312"
          c:name="Spring Framework" />

   <context:annotation-config />

    <bean name="student" class="org.dhiren.spring.annotation.models.Student" />

</beans>